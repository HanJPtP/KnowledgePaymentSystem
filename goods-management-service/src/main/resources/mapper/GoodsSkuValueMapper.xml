<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniu.outlet.mysql.mapper.GoodsSkuValueMapper">

    <resultMap id="BaseResultMap" type="com.woniu.outlet.dao.mysql.po.GoodsSkuValue">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="skuId" column="sku_id" jdbcType="BIGINT"/>
            <result property="valueId" column="value_id" jdbcType="BIGINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,sku_id,value_id
    </sql>

    <insert id="saveGoodsSkuAndValueList" parameterType="com.woniu.outlet.dao.mysql.po.GoodsSkuValue">
        insert into goods_sku_value (sku_id, value_id, id) values
           <foreach collection="goodsSkuValueList" item="goodsSkuValue" separator=",">
                (#{goodsSkuValue.skuId}, #{goodsSkuValue.valueId}, #{goodsSkuValue.id})
           </foreach>
    </insert>
</mapper>
